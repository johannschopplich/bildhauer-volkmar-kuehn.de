{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "unocss": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run unocss -- --watch\" \"vite\"",
    "build": "npm run unocss && vite build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier \"src/**/*\" --write"
  },
  "dependencies": {
    "duecss": "^0.32.0",
    "medium-zoom": "^1.1.0",
    "unlazy": "^0.12.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@types/node": "^22.13.5",
    "@unocss/cli": "^66.0.0",
    "@unocss/preset-wind3": "66.1.0-beta.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "sass": "^1.85.1",
    "shx": "^0.3.4",
    "typescript": "^5.7.3",
    "unocss": "^66.0.0",
    "vite": "^6.2.0",
    "vite-plugin-full-reload": "^1.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
}
