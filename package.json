{
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "scripts": {
    "unocss": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run unocss -- --watch\" \"vite\"",
    "build": "npm run unocss && vite build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier \"src/**/*\" --write"
  },
  "dependencies": {
    "duecss": "^0.31.2",
    "medium-zoom": "^1.0.8",
    "unlazy": "^0.10.1"
  },
  "devDependencies": {
    "@byjohann/eslint-config": "^0.1.2",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@types/node": "^20.6.1",
    "@unocss/cli": "^0.55.7",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "fontaine": "^0.4.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "shx": "^0.3.4",
    "typescript": "^5.2.2",
    "unocss": "^0.55.7",
    "vite": "^4.4.9",
    "vite-plugin-full-reload": "^1.0.5"
  }
}
